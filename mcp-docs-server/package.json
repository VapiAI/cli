{
  "name": "@vapi-ai/mcp-docs-server",
  "version": "0.1.1",
  "description": "Model Context Protocol server providing access to Vapi documentation, examples, and knowledge base",
  "main": "dist/index.js",
  "bin": {
    "vapi-mcp-docs-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "watch": "tsx watch src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "vapi",
    "voice-ai",
    "documentation",
    "ai-assistant",
    "llm-tools"
  ],
  "author": "Vapi AI <support@vapi.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/VapiAI/mcp-docs-server.git"
  },
  "homepage": "https://docs.vapi.ai",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.7.0",
    "cheerio": "^1.0.0",
    "gray-matter": "^4.0.3",
    "fuse.js": "^7.0.0",
    "marked": "^12.0.0",
    "yaml": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
} 